

:- sorts
	var.

:- objects
	1..4, foo :: var.

:- constants
	mv :: var;
	b(var) :: rigid.
	mv2(var) :: var.

:- variables
	V :: var.

% caused {mv}.
caused {mv=V}.
%caused {-(mv=V)}.
%caused {not (mv=V)}.
%caused {-mv}.
%caused {not mv}.
caused {b(V)}.
caused {-b(V)}.
caused {not b(V)}.
%caused false if {mv=V}.
caused {mv2(V)=V}.

